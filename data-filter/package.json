        "sequelize": "^6.6.0",
        "sequelize-typescript": "^2.1.0"
    },
    "devDependencies": {
        "@nestjs/common": "^8.0.7",
        "@nestjs/core": "^8.0.7",
        "@nestjs/testing": "^8.0.7",
        "@recursyve/nestjs-common": "^8.0.0",
        "@types/express": "^4.17.13",
