{
    "name": "@recursyve/nestjs-access-control",
    "version": "8.0.0-beta.0",
    "description": "NestJs AccessControl library",
    "main": "index.js",
    "scripts": {
        "watch": "gulp",
        "build": "gulp build",
        "build:lib": "gulp build-lib",
        "lint": "gulp lint",
        "publish-package": "npm run build:lib && npm publish dist --access public",
        "publish-package:beta": "npm run publish-package -- --tag beta --access public",
        "publish-package:dry-run": "npm run publish-package -- --dry-run"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Recursyve/nestjs-librairies/access-control"
    },
    "author": "Recursyve",
    "license": "MIT",
    "peerDependencies": {
        "@nestjs/common": "^8.0.0",
        "@nestjs/core": "^8.0.0",
        "@nestjs/cqrs": "^8.0.0",
        "@nestjs/sequelize": "^8.0.0",
        "@recursyve/nestjs-redis": "^8.0.0",
        "sequelize": "^6.0.0",
        "sequelize-typescript": "^2.1.0"
    },
    "devDependencies": {
        "@nestjs/common": "^8.0.7",
        "@nestjs/core": "^8.0.7",
        "@nestjs/cqrs": "^8.0.0",
        "@nestjs/sequelize": "^8.0.0",
        "@nestjs/testing": "^8.0.7",
        "@recursyve/nestjs-redis": "^8.0.0",
        "@types/jest": "^27.0.2",
        "gulp": "^4.0.2",
        "gulp-copy": "^4.0.1",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "jest": "^27.2.2",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.3.0",
        "sequelize": "6.6.2",
        "sequelize-typescript": "^2.1.0",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.2.1",
        "typescript": "^4.4.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "lib",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "./coverage",
        "testEnvironment": "node"
    }
}
