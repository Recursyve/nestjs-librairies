{
    "name": "@recursyve/nestjs-redis",
    "version": "8.2.0",
    "description": "Recursyve's NestJs Redis client",
    "main": "index.js",
    "scripts": {
        "watch": "gulp",
        "build": "gulp build",
        "build:lib": "gulp build-lib",
        "lint": "gulp lint",
        "publish-package": "npm run build:lib && npm publish ./dist --access public",
        "publish-package:beta": "npm run publish-package -- --tag beta --access public",
        "publish-package:dry-run": "npm run publish-package -- --dry-run"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Recursyve/nestjs-librairies/redis"
    },
    "author": "Recursyve",
    "license": "MIT",
    "peerDependencies": {
        "@nestjs/common": ">=8.0.0",
        "@nestjs/core": ">=8.0.0",
        "@nestjs/terminus": ">=8.0.0"
    },
    "dependencies": {
        "ioredis": "^4.28.4"
    },
    "devDependencies": {
        "@nestjs/common": "^8.2.6",
        "@nestjs/core": "^8.2.6",
        "@nestjs/terminus": "^8.0.4",
        "@types/ioredis": "^4.28.7",
        "@types/jest": "^27.4.0",
        "@types/node": "^16.11.22",
        "gulp": "^4.0.2",
        "gulp-copy": "^4.0.1",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "jest": "^27.5.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.5.2",
        "ts-jest": "^27.1.3",
        "typescript": "^4.5.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "lib",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "./coverage",
        "testEnvironment": "node"
    }
}
