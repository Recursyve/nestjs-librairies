    },
    "dependencies": {
        "csv-stringify": "^5.3.6",
        "xlsx": "^0.15.6"
    },
    "devDependencies": {
        "@nestjs/common": "^7.0.0",
        "@nestjs/core": "^7.0.0",
        "@nestjs/cqrs": "^6.1.0",
