        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "express": "^4.17.1",
        "gulp": "^4.0.2",
        "gulp-copy": "^4.0.1",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-typescript": "^5.0.1",
