  "peerDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@recursyve/nestjs-common": "^7.0.0",
    "moment": "^2.24.0",
    "sequelize": "^5.21.5",
    "sequelize-typescript": "^1.1.0"
  },
  "devDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/cqrs": "^6.1.0",
    "@nestjs/testing": "^7.0.8",
    "@recursyve/nestjs-common": "^7.0.0",
    "@types/bluebird": "*",
    "@types/express": "^4.17.9",
    "@types/jest": "^25.1.4",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.12.36",
    "@types/xlsx": "0.0.36",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-copy": "^4.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "jest": "^25.2.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "sequelize": "^5.21.5",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^4.1.1",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
