        "sequelize": "^6.6.0",
        "sequelize-typescript": "^2.1.0"
    },
    "devDependencies": {
        "@nestjs/common": "^7.6.15",
        "@nestjs/core": "^7.6.15",
        "@nestjs/testing": "^7.6.15",
        "@recursyve/nestjs-common": "^7.0.3",
        "@types/bluebird": "^3.5.33",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.23",
