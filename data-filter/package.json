{
  "name": "@recursyve/nestjs-data-filter",
  "version": "1.2.0",
  "description": "NestJs DataFilter library",
  "main": "index.js",
  "scripts": {
    "watch": "gulp",
    "build": "gulp build",
    "build:lib": "gulp build-lib",
    "lint": "gulp lint",
    "publish-package": "npm run build:lib && npm publish dist --access public",
    "publish-package:beta": "npm run publish-package -- --tag beta --access public",
    "publish-package:dry-run": "npm run publish-package -- --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Recursyve/nestjs-librairies/data-filter"
  },
  "author": "Recursyve",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/cqrs": "^6.1.0",
    "@recursyve/nestjs-access-control": "^1.0.3",
    "@recursyve/nestjs-common": "^1.0.4",
    "@recursyve/nestjs-redis": "^1.0.2",
    "sequelize": "^5.18.4",
    "sequelize-typescript": "^1.1.0"
  },
  "devDependencies": {
    "@nestjs/common": "^6.11.6",
    "@nestjs/core": "^6.11.6",
    "@nestjs/cqrs": "^6.1.0",
    "@recursyve/nestjs-access-control": "^1.0.3",
    "@recursyve/nestjs-common": "^1.0.4",
    "@recursyve/nestjs-redis": "^1.0.2",
    "@types/jest": "^25.1.3",
    "@types/moment": "^2.13.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "gulp": "^4.0.2",
    "gulp-copy": "^4.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "jest": "^25.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "sequelize": "^5.18.4",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^4.1.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
