{
    "name": "@recursyve/nestjs-access-control",
    "version": "7.2.0",
    "description": "NestJs AccessControl library",
    "main": "index.js",
    "scripts": {
        "watch": "gulp",
        "build": "gulp build",
        "build:lib": "gulp build-lib",
        "lint": "gulp lint",
        "publish-package": "npm run build:lib && npm publish dist --access public",
        "publish-package:beta": "npm run publish-package -- --tag beta --access public",
        "publish-package:dry-run": "npm run publish-package -- --dry-run"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Recursyve/nestjs-librairies/access-control"
    },
    "author": "Recursyve",
    "license": "MIT",
    "peerDependencies": {
        "@nestjs/common": "^7.0.0",
        "@nestjs/core": "^7.0.0",
        "@nestjs/cqrs": "^6.1.0",
        "@nestjs/sequelize": "^0.1.1",
        "@recursyve/nestjs-redis": "^7.0.0",
        "sequelize": "^5.21.5",
        "sequelize-typescript": "^1.1.0"
    },
    "devDependencies": {
        "@nestjs/common": "^7.0.0",
        "@nestjs/core": "^7.0.0",
        "@nestjs/cqrs": "^6.1.0",
        "@nestjs/sequelize": "^0.1.1",
        "@nestjs/testing": "^7.4.4",
        "@recursyve/nestjs-redis": "^7.0.0",
        "@types/bluebird": "*",
        "@types/validator": "*",
        "gulp": "^4.0.2",
        "gulp-copy": "^4.0.1",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-typescript": "^5.0.1",
        "jest": "^26.4.2",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.5.4",
        "sequelize": "^5.21.5",
        "sequelize-typescript": "^1.1.0",
        "ts-jest": "^26.4.0",
        "ts-node": "^9.0.0",
        "typescript": "^3.8.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "lib",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "./coverage",
        "testEnvironment": "node"
    }
}
