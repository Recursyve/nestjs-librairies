    "license": "MIT",
    "peerDependencies": {
        "@nestjs/common": ">=8.0.0",
        "@nestjs/core": ">=8.0.0",
        "@recursyve/nestjs-common": ">=8.0.0",
        "sequelize": ">=6.19.0",
        "sequelize-typescript": ">=2.1.0",
        "date-fns": "^2.28.0",
        "date-fns-tz": "^1.3.4"
    },
    "devDependencies": {
        "@nestjs/common": "^8.2.6",
        "@nestjs/core": "^8.2.6",
        "@nestjs/testing": "^8.2.6",
        "@recursyve/nestjs-common": "^8.0.4",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.15",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "date-fns": "^2.28.0",
        "date-fns-tz": "^1.3.4",
        "express": "^4.17.2",
        "gulp": "^4.0.2",
        "gulp-copy": "^4.0.1",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "jest": "^27.5.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.5.2",
        "sequelize": "^6.19.0",
        "sequelize-typescript": "^2.1.3",
        "sqlite3": "^5.0.2",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "lib",
